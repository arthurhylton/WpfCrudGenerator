<Window x:Class="WpfCrudGenerator.WndTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCrudGenerator"
        mc:Ignorable="d"
        Title="WndTest" Height="300" Width="300">
    <DockPanel>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Width="100" Text="CSharpCode:" />
                <TextBox Width="200" Text="{Binding CSharpCode}" />
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Width="100" Text="XamlCode:" />
                <CheckBox IsChecked="{Binding XamlCode, Mode=TwoWay}" />
            </StackPanel>

        </StackPanel>
    </DockPanel>
</Window>
